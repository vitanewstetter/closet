{
  "name": "node-personal-website",
  "version": "0.0.1",
  "description": "Example Begin personal website",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec NODE_ENV=testing ARC_LOCAL=1 npx sandbox",
    "lint": "eslint src --ignore-pattern node_modules --fix"
  },
  "nodemonConfig": {
    "watch": [
      "src/shared",
      "src/views"
    ],
    "ext": "css,js,json,md,mjs"
  },
  "keywords": [
    "begin",
    "serverless",
    "personal website",
    "example"
  ],
  "author": "@dam",
  "license": "Apache-2.0",
  "devDependencies": {
    "@architect/architect": "^5.9.24",
    "eslint": "^5.16.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.2",
    "tiny-json-http": "^7.1.2"
  }
}